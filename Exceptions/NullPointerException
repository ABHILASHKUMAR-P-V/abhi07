
import java.io.*;

class nullpointer
{
	public static void main (String[] args)
	{
		
		String ptr = null;

		try
		{
			if ("jala".equals(ptr))
				System.out.print("Same");
			else
				System.out.print("Not Same");			
		}
		catch(NullPointerException e)
		{
			System.out.print("Caught NullPointerException");
		}
	}
}
