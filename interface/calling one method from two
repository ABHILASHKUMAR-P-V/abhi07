packagecom.core;
interface MyInterface
{
  
   public void method1();
   public void method2();
}
class Demo implements MyInterface
{
 
   public void method1()
   {
	System.out.println("implementation of method1");
   }
   
   public static void main(String arg[])
   {
	MyInterface obj = new Demo();
	obj.method1();
   }
}
